<?xml version="1.0" encoding="UTF-8"?>
<vconfkeylists>
  <filedef name="__VCONF_INTERNAL_MOBILE_HOTSPOT_KEYS_H__"/>
  <vconfkey type="int32" name="memory/mobile_hotspot/mode" layer="memory" readPriv="http://tizen.org/privilege/internal/default/public" writePriv="http://tizen.org/privilege/tethering.admin">
    <header>VCONFKEY_MOBILE_HOTSPOT_MODE</header>
    <val>0</val>
    <creator>Seonah Moon</creator>
    <email>seonah1.moon@samsung.com</email>
    <comment><![CDATA[ To notify which tethering type is enabled ]]></comment>
    <enumlist>
      <enumdata value="0x00">VCONFKEY_MOBILE_HOTSPOT_MODE_NONE     </enumdata>
      <enumdata value="0x01">VCONFKEY_MOBILE_HOTSPOT_MODE_WIFI     </enumdata>
      <enumdata value="0x02">VCONFKEY_MOBILE_HOTSPOT_MODE_USB      </enumdata>
      <enumdata value="0x04">VCONFKEY_MOBILE_HOTSPOT_MODE_BT       </enumdata>
      <enumdata value="0x08">VCONFKEY_MOBILE_HOTSPOT_MODE_WIFI_AP  </enumdata>
    </enumlist>
  </vconfkey>
  <vconfkey type="int32" name="memory/mobile_hotspot/connected_device" layer="memory" readPriv="http://tizen.org/privilege/internal/default/public" writePriv="http://tizen.org/privilege/tethering.admin">
    <header>VCONFKEY_MOBILE_HOTSPOT_CONNECTED_DEVICE</header>
    <val>0</val>
    <creator>Seonah Moon</creator>
    <email>seonah1.moon@samsung.com</email>
    <comment><![CDATA[ To notify clients which are connected through tethering ]]></comment>
  </vconfkey>
  <vconfkey type="int32" name="db/mobile_hotspot/security" layer="system" readPriv="http://tizen.org/privilege/internal/default/public" writePriv="http://tizen.org/privilege/tethering.admin">
    <header>VCONFKEY_MOBILE_HOTSPOT_SECURITY</header>
    <val>1</val>
    <creator>Seonah Moon</creator>
    <email>seonah1.moon@samsung.com</email>
    <comment><![CDATA[ To notify tethering security type ]]></comment>
    <enumlist>
      <enumdata value="0x00">VCONFKEY_MOBILE_AP_SECURITY_OFF </enumdata>
      <enumdata>VCONFKEY_MOBILE_AP_SECURITY_ON  </enumdata>
    </enumlist>
  </vconfkey>
  <vconfkey type="int32" name="db/mobile_hotspot/hide" layer="system" readPriv="http://tizen.org/privilege/internal/default/public" writePriv="http://tizen.org/privilege/tethering.admin">
    <header>VCONFKEY_MOBILE_HOTSPOT_HIDE</header>
    <val>0</val>
    <creator>Seonah Moon</creator>
    <email>seonah1.moon@samsung.com</email>
    <comment><![CDATA[ To notify tethering visibility mode(visible/invisible) ]]></comment>
    <enumlist>
      <enumdata value="0x00">VCONFKEY_MOBILE_AP_HIDE_OFF </enumdata>
      <enumdata>VCONFKEY_MOBILE_AP_HIDE_ON  </enumdata>
    </enumlist>
  </vconfkey>
  <vconfkey type="string" name="memory/private/mobileap-agent/ssid" layer="memory" readPriv="http://tizen.org/privilege/internal/default/public" writePriv="http://tizen.org/privilege/tethering.admin">
    <header>VCONFKEY_MOBILE_HOTSPOT_SSID</header>
    <val/>
    <creator>Seonah Moon</creator>
    <email>seonah1.moon@samsung.com</email>
    <comment><![CDATA[ Thethering device's SSID ]]></comment>
  </vconfkey>
  <vconfkey type="bool" name="db/private/libug-setting-mobileap-efl/prev_wifi_status" layer="system" readPriv="http://tizen.org/privilege/internal/default/platform" writePriv="http://tizen.org/privilege/internal/default/platform">
