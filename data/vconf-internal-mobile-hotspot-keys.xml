  <vconfkey type="int32" name="file/private/libug-setting-mobileap-efl/usb_popup_check_box_status" layer="system" readPriv="http://tizen.org/privilege/internal/default/platform" writePriv="http://tizen.org/privilege/internal/default/platform">
    <header>VCONFKEY_MOBILE_HOTSPOT_USB_POPUP_CHECK_BOX_STATUS</header>
    <val>0</val>
    <creator>Seonah Moon</creator>
    <email>seonah1.moon@samsung.com</email>
    <comment><![CDATA[ USB popup check-box status for enabling USB tethering ]]></comment>
  </vconfkey>
  <vconfkey type="int32" name="memory/private/libug-setting-mobileap-efl/trying_usb_tethering" layer="memory" readPriv="http://tizen.org/privilege/internal/default/public" writePriv="http://tizen.org/privilege/tethering.admin">
    <header>VCONFKEY_MOBILE_HOTSPOT_TRYING_USB_TETHERING</header>
    <val>0</val>
    <creator>Seonah Moon</creator>
    <email>seonah1.moon@samsung.com</email>
